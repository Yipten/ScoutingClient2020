<Window x:Class="ScoutingClient2020.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		mc:Ignorable="d"
		Title="Scouting Client 2020"
		Height="650"
		Width="650"
		WindowState="Maximized"
		ResizeMode="CanResize"
		Style="{StaticResource Default}">
	<Grid>
		<TabControl>
			<TabItem Header="Team Stats">
				<Frame Source="Views/TeamStatsView.xaml" />
			</TabItem>
			<TabItem Header="Team Trends">
				<Frame Source="Views/TeamTrendsView.xaml" />
			</TabItem>
			<TabItem Header="Match History">
				<Frame Source="Views/MatchHistoryView.xaml" />
			</TabItem>
			<TabItem Header="Match Predictions">
				<Frame Source="Views/MatchPredictionsView.xaml" />
			</TabItem>
			<TabItem Header="Scoring">
				<Frame Source="Views/ScoringView.xaml" />
			</TabItem>
			<TabItem Header="Data Management">
				<Frame Source="Views/DataManagementView.xaml" />
			</TabItem>
		</TabControl>
	</Grid>
</Window>
